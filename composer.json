{
  "name": "fabiopellati/metronic-theme",
  "description": "elementi di view per l'implementazione del tema metronic",
  "license": "proprietary",
  "authors": [
    {
      "name": "Fabio Pellati",
      "email": "fabio.pellati@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "laminas/laminas-view": "^2.9.0",
    "rwoverdijk/assetmanager": "^1.7",
    "laminas/laminas-navigation": "^2.8.2",
    "laminas/laminas-dependency-plugin": "^2.1"
  },
  "require-dev": {
    "laminas/laminas-test": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "MetronicTheme\\": "src/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  },
  "extra": {
    "laminas": {
      "component": [
        "MetronicTheme",
        "Laminas\\Navigation",
        "AssetManager"
      ]
    }
  }
}
